<header class="masthead" style="background-image: url('/assets/img/<%= bgImage %>')">
    <div class="container position-relative px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-md-10 col-lg-8 col-xl-7">
                <div class="site-heading">
                    <h1><%= pageHeaderH1 %></h1>
                    <% if(typeof locals.postPage != 'undefined' && postPage) { %>
                        <span class="meta">
                            Posted by
                            <a href="#!">Start Bootstrap</a>
                            on August 24, 2023
                        </span>
                    <% } else { %>
                        <span class="subheading"><%= pageHeaderSub %></span>
                    <% } %>
                </div>
            </div>
        </div>
    </div>
</header>
